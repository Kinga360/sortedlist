import java.util.Queue;
import java.util.Iterator;
import java.util.LinkedList;
import java.lang.reflect.Array;
import java.io.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;



import java.util.Scanner;
public class week3inter1 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter a value.");
		String input = scan.nextLine();
		Queue<String> numbers = new LinkedList<String>();
		numbers.add(input);
		System.out.println("Enter another value.");
		String input1 = scan.nextLine();
		numbers.add(input1);
		System.out.println("Enter another value.");
		String input2 = scan.nextLine();
		numbers.add(input2);
		
		
		System.out.println("There are the element(s)" +numbers + " in this list");
		
		
		System.out.println("Do you want elements to be removed?");
		String answer = scan.nextLine();
		if (answer.equals("yes")) {
			System.out.println("How many elements you want to be removed?" );
			int index = scan.nextInt();
			for( int i = 0; i < index; i++) {  
			numbers.remove();
			}
		}
		System.out.println("There are the element(s)" +numbers + " in this list");
		
		}
	
	}


